{
  "name": "My SaltCorn Bookmarks",
  "version": "1.1.0",
  "description": "Josiah Likes Kidss",
  "permissions": ["notifications", "storage", "tabs","sidePanel"],
  "host_permissions": ["*://*.saltcorn.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.saltcorn.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "side_panel":{
    "default_path":"sidebar.html"
  },
      "web_accessible_resources": [{
    "resources": ["images/image1.jpg", "images/image2.jpg", "images/image3.jpg", "images/image4.jpg"],
    "matches": ["<all_urls>"]}],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "icons": {
    "16": "assets/ext-icon.png",
    "48": "assets/ext-icon.png",
    "128": "assets/ext-icon.png"
  },
    "default_title": "Hi"

  
  },
  "manifest_version": 3
}
